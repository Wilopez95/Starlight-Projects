{
  "name": "@starlightpro/hauling-frontend",
  "private": true,
  "version": "1.8.6",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./dist/client/index.js",
  "scripts": {
    "build:beta": "cross-env NODE_ENV=beta NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:demo": "cross-env NODE_ENV=demo NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:dev": "cross-env NODE_ENV=dev NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:dev3": "cross-env NODE_ENV=dev3 NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:prod": "yarn build:production",
    "build:production": "cross-env NODE_ENV=production SENTRY_DRY_RUN=false webpack --config webpack/production.js",
    "build:qa": "cross-env NODE_ENV=qa NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:qa1": "cross-env NODE_ENV=qa1 NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:qa2": "cross-env NODE_ENV=qa2 NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:staging": "cross-env NODE_ENV=staging GENERATE_SOURCEMAP=true  NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build:uat": "cross-env NODE_ENV=uat NODE_OPTIONS='--max_old_space_size=4096' SENTRY_DRY_RUN=true webpack --config webpack/production.js",
    "build": "yarn build:production",
    "clean": "rimraf ./dist",
    "eslint": "eslint",
    "fixAll": "yarn format --write && yarn lint:ts --fix",
    "format": "prettier src/**/* -c",
    "i18nSync:exec": "ts-node --project scripts/i18n/tsconfig.json scripts/i18n/src/index.ts",
    "i18nSync:pull": "yarn run i18nSync:exec -- --pull",
    "i18nSync:push": "yarn run i18nSync:exec -- --push",
    "jest": "jest",
    "lint-staged": "lint-staged",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:ts": "eslint .",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "mp": "node ./scripts/multiPlatform.js",
    "prepare": "husky install && yarn mp chmod +x ./.husky/pre-commit && yarn mp chmod +x ./.husky/pre-push",
    "prettier": "prettier",
    "start:demo": "cross-env NODE_ENV=demo webpack-dev-server --config webpack/development.js",
    "start:dev": "cross-env NODE_ENV=dev webpack-dev-server --config webpack/development.js",
    "start:dev3": "cross-env NODE_ENV=dev3 webpack-dev-server --config webpack/development.js",
    "start:local": "cross-env NODE_ENV=local webpack-dev-server --config webpack/development.js",
    "start:qa1": "cross-env NODE_ENV=qa1 webpack-dev-server --config webpack/development.js",
    "start:staging": "cross-env GENERATE_SOURCEMAP=true NODE_ENV=staging webpack-dev-server --config webpack/development.js",
    "start": "yarn start:local",
    "stylelint": "stylelint",
    "test:unit": "jest --runInBand --no-cache --detectOpenHandles --config jest.unit.config.js",
    "test": "yarn test:unit"
  },
