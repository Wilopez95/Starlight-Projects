{
  "name": "starlight-route-planner-backend",
  "private": true,
  "version": "1.6.0",
  "description": "Starlight Route Planner service",
  "engines": {
    "node": ">=16.14.0"
  },
  "type": "module",
  "main": "server.js",
  "scripts": {
    "coverage": "rm -rf ./coverage && jest --runInBand --no-cache --verbose --testTimeout=10000 --detectOpenHandles --coverage || true",
    "db:reset": "yarn migrate rollback && yarn migrate up && yarn seed run",
    "fix": "yarn prettier:fix && yarn lint:fix",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "migrate": "node scripts/migrate.js",
    "mq:reset": "node scripts/resetMq.js",
    "pm2:start": "pm2 start server.js -f --name test-billing-server --wait-ready --listen-timeout 20000",
    "pm2:stop": "pm2 stop test-billing-server && pm2 delete test-billing-server",
    "posttest": "yarn pm2:stop && yarn db:reset && yarn mq:reset",
    "prepare": "husky install .husky",
    "pretest": "yarn db:reset && yarn mq:reset && yarn pm2:start",
    "prettier:fix": "prettier  --write . -c",
    "prettier": "prettier --check '**/*.js'",
    "pretty-quick": "pretty-quick",
    "seed": "node scripts/seed.js",
    "snapshot": "node scripts/snapshot.js",
    "start:dev": "nodemon server.js | pino-pretty",
    "start:local": "nodemon server.js | pino-pretty",
    "start:skip": "node server.js --skip-migration --skip-mq-setup | pino-pretty",
    "start": "node server.js | pino-pretty",
    "test:unit": "jest --config jest.unit.config.js",
    "test": "jest --runInBand --no-cache --verbose --testTimeout=10000 --detectOpenHandles || true",
    "upgrade": "ncu --loglevel verbose --packageFile package.json -u"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.12.0",
    "@joi/date": "^2.1.0",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@sentry/integrations": "7.41.0",
    "@sentry/node": "7.41.0",
    "amqplib": "^0.6.0",
    "apollo-datasource-rest": "^0.10.0",
    "apollo-server-core": "^2.19.2",
    "apollo-server-koa": "^2.19.2",
    "aws-sdk": "^2.835.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "ci-info": "^3.2.0",
    "cloudinary": "^1.25.2",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "dotenv": "^8.2.0",
    "dummy-json": "^3.0.1",
    "exceljs": "^4.2.0",
    "form-data": "^4.0.0",
    "graphql": "^15.3.0",
    "graphql-playground-middleware-koa": "^1.6.21",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.2",
    "graphql-upload": "^11.0.0",
    "http-status": "^1.4.2",
    "ioredis": "^4.19.4",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "koa": "^2.13.0",
    "koa-basic-auth": "^4.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "lodash": "^4.17.20",
    "minimatch": "^3.0.4",
    "nanoid": "^3.1.12",
    "node-cache": "^5.1.2",
    "objection": "^2.2.3",
    "paseto": "^1.0.8",
    "pg": "^8.4.1",
    "pino": "8.11.0",
    "pino-datadog-transport": "1.3.0",
    "pino-pretty": "9.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@starlightpro/eslint-config-starlight": "^0.5.0-rc.12",
    "babel-jest": "^26.5.2",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "eslint": "8.31.0",
    "husky": "8.0.3",
    "jest": "^26.5.3",
    "lint-staged": "13.1.0",
    "nodemon": "^2.0.6",
    "pm2": "^4.5.0",
    "prettier": "2.8.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.1.3",
    "yargs": "^16.1.0"
  }
}
