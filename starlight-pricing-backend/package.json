{
  "name": "starlight-base-backend",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install .husky",
    "typeorm": "typeorm-ts-node-commonjs",
    "build": "tsc",
    "run-dist": "node --max-old-space-size=8094 --max-http-header-size=48000 ./dist/src/app.js",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint --config .eslintrc.js ./src ./scripts",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "test": "jest",
    "migrate": "yarn typeorm migration:run -d ./src/data-source.ts",
    "migrate:down": "yarn typeorm migration:revert -d ./src/data-source.ts",
    "start": "ts-node src/app.ts",
    "start:dev": " yarn nodemon src/app.ts",
    "start:local": " yarn nodemon src/app.ts",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -d ./src/data-source.ts",
    "migration:generate": "yarn typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/database/migrations/default",
    "migration:run": "yarn typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "prettier:fix": "prettier  --write . -c",
    "prettier": "prettier --check '**/*.ts'"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@opensearch-project/opensearch": "^2.0.0",
    "amqp-ts": "^1.8.0",
    "amqplib": "^0.8.0",
    "axios": "^0.21.1",
    "ci-info": "3.1.1",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "dotenv": "16.0.3",
    "graphql": "^16.4.0",
    "graphql-scalars": "^1.17.0",
    "ioredis": "4.23.0",
    "joi": "^17.6.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-graphql": "^0.12.0",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-swagger-decorator": "^1.8.5",
    "lodash": "^4.17.21",
    "nanoid": "3.1.22",
    "nodemon": "2.0.20",
    "paseto": "2.1.1",
    "pg": "^8.4.0",
    "pino": "^8.7.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.2",
    "ts-node-dev": "2.0.0",
    "typeorm": "0.3.7",
    "winston": "3.8.2",
    "winston-loggly-bulk": "3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@starlightpro/eslint-config-starlight": "0.5.0-rc.12",
    "@types/amqplib": "^0.8.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.4.1",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "4.3.7",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.1.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.31.0",
    "graphql-iso-date": "^3.6.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.1.2",
    "nodemon": "2.0.20",
    "prettier": "2.8.2",
    "ts-node": "10.9.1",
    "tsc": "^2.0.4",
    "tslint": "^6.1.3",
    "typescript": "4.9.4"
  }
}
