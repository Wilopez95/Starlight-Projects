{
  "name": "starlight-hauling-backend",
  "private": true,
  "version": "1.8.6",
  "description": "Starlight Hauling Backend",
  "engines": {
    "node": ">=14.18.0"
  },
  "type": "module",
  "scripts": {
    "coverage:integration": "rm -rf ./coverage && jest --runInBand --no-cache --verbose --testTimeout=10000 --detectOpenHandles --coverage --config jest.integration.config.js",
    "coverage:unit": "rm -rf ./coverage && jest --runInBand --no-cache --verbose --testTimeout=10000 --detectOpenHandles --coverage --config jest.unit.config.js",
    "coverage": "rm -rf ./coverage && jest --runInBand --no-cache --verbose --testTimeout=10000 --detectOpenHandles --coverage || true",
    "cpy:backup": "cpy .env ./ --rename=backup.env",
    "cpy:e2e": "cpy e2e.env ./ --rename=.env",
    "cpy:local": "cpy local.env ./ --rename=.env",
    "cpy:revert": "cpy backup.env ./ --rename=.env && rm -f ./backup.env",
    "db:init": "node scripts/initDb.js",
    "db:reset": "yarn migrate rollback && yarn migrate up && yarn seed run",
    "dev": "node server.js --skip-migration",
    "elastic:reset": "node scripts/resetElastic.js",
    "jest": "jest",
    "lint-staged": "lint-staged",
    "lint": "eslint --config .eslintrc.json .",
    "lint:fix": "eslint --config .eslintrc.json . --fix",
    "migrate": "node scripts/migrate.js",
    "mp": "node ./scripts/multiPlatform.js",
    "mq:reset": "node scripts/resetMq.js",
    "mq:sync": "node scripts/syncMq.js",
    "pm2:start": "pm2 start server.js --node-args=\"--skip-migration\" -f --name test-server --wait-ready --listen-timeout 20000",
    "pm2:stop": "pm2 stop test-server && pm2 delete test-server",
    "prepare": "husky install .husky",
    "prettier": "prettier --check '**/*.js'",
    "prettier:fx": "prettier --write",
    "provision:e2e": "yarn mp chmod +x ./scripts/haulingBeE2EProvisioning.sh & ./scripts/haulingBeE2EProvisioning.sh",
    "provision:local": "yarn mp chmod +x ./scripts/haulingBeLocalDevProvisioning.sh & ./scripts/haulingBeLocalDevProvisioning.sh",
    "seed": "node scripts/seed.js",
    "snapshot": "node scripts/snapshot.js",
    "start:debug": "nodemon --inspect server.js",
    "start:dev": "nodemon server.js",
    "start:local": "nodemon server.js",
    "start:es-resync": "node server.js --es-resync",
    "start:skip": "node server.js --skip-migration --skip-es --skip-mq-setup",
    "start": "node server.js",
    "test:e2e:price": "jest --runInBand --verbose --config jest.price.e2e.config.js --detectOpenHandles",
    "test:e2e:run": "jest --runInBand --verbose --config jest.e2e.config.js --detectOpenHandles",
    "test:e2e:subs": "jest --runInBand --verbose --config jest.subs.e2e.config.js --detectOpenHandles",
    "test:e2e": "yarn test:e2e:run",
    "test:integration": "jest --config jest.integration.config.js",
    "test:unit": "jest --config jest.unit.config.js",
    "test": "yarn test:e2e",
    "upgrade": "ncu --loglevel verbose --packageFile package.json -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starlightpro/starlight-hauling-backend"
  },
  "homepage": "https://github.com/Starlightpro/starlight-hauling-backend#readme",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@mapbox/mapbox-sdk": "^0.12.1",
    "@opensearch-project/opensearch": "^1.0.0",
    "@sendgrid/client": "^7.4.2",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/node": "7.41.0",
    "@socket.io/redis-adapter": "^7.0.0",
    "@starlightpro/osm-helpers": "^0.1.0",
    "amqplib": "^0.7.1",
    "aws-sdk": "^2.882.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "busboy": "^0.3.1",
    "ci-info": "^3.1.1",
    "cloudinary": "^1.26.2",
    "convert-units": "^3.0.0-beta.2",
    "cookies": "^0.8.0",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "dd-trace": "^2.4.2",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "faker": "5.5.2",
    "formidable": "^1.2.2",
    "http-status": "^1.5.0",
    "ioredis": "^4.23.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.9",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.1.0",
    "koa-logger": "^3.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.22",
    "nock": "^13.1.1",
    "node-cache": "^5.1.2",
    "npm-run-all": "^4.1.5",
    "paseto": "^2.1.1",
    "pg": "^8.6.0",
    "pg-query-stream": "^4.1.0",
    "pino": "^6.11.2",
    "pino-pretty": "^4.7.1",
    "socket.io": "^4.2.0",
    "stat-mode": "^1.0.0",
    "twilio": "^3.64.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@starlightpro/eslint-config-starlight": "0.5.0-rc.12",
    "@types/node": "^18.7.18",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-import-meta": "^2.0.0",
    "cpy": "^7.3.0",
    "cpy-cli": "^2.0.0",
    "dummy-json": "^3.0.4",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pm2": "^5.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.1.3",
    "typescript": "^4.8.3",
    "yargs": "^17.2.1"
  },
  "resolutions": {
    "normalize-url": "^6.0.1"
  }
}
