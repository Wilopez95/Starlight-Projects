{
  "name": "starlight-user-management-service",
  "version": "1.8.8",
  "private": true,
  "type": "commonjs",
  "repository": "https://github.com/Starlightpro/starlight-user-management-service",
  "engines": {
    "node": ">=12.12.0"
  },
  "main": "dist/src/main.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "cognito:serve": "http-server cognito/ -c-1",
    "cognito:upload-customizations": "ts-node ./scripts/cognitoUploadCustomizations.ts",
    "generate-key": "ts-node ./scripts/generateKey.ts",
    "generate-schema": "ts-node ./scripts/generateSchema.ts",
    "generate-service-keys": "ts-node ./scripts/generateServiceKeys.ts",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint --config .eslintrc.js . --fix",
    "lint:eslint": "eslint --config .eslintrc.js ./src ./scripts",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "prepare": "husky install .husky",
    "prettier:fix": "prettier --ignore-unknown --write",
    "prettier": "prettier --check '**/*.js'",
    "pretty-quick": "pretty-quick",
    "setup:admin": "ts-node ./scripts/setupAdmin.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts | pino-pretty",
    "start:local": "ts-node-dev --respawn --transpile-only src/main.ts | pino-pretty",
    "start": "node --enable-source-maps ./dist/src/main.js",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@apollo/federation": "^0.32.0",
    "@koa/cors": "^3.1.1",
    "@koa/router": "^10.1.1",
    "@opensearch-project/opensearch": "^1.0.0",
    "@sentry/integrations": "^7.45.0",
    "@sentry/node": "^7.45.0",
    "@sentry/types": "^7.45.0",
    "amqplib": "^0.8.0",
    "apollo-graphql": "^0.9.3",
    "apollo-server-core": "^3.3.0",
    "aws-sdk": "^2.991.0",
    "ci-info": "^3.2.0",
    "class-validator": "^0.13.1",
    "dd-trace": "3.16.0",
    "dotenv": "^16.0.0",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "http-graceful-shutdown": "^3.1.4",
    "http-status": "^1.5.0",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "nanoid": "^3.3.2",
    "node-fetch": "^2.6.0",
    "paseto": "^1.0.8",
    "pg": "~8.7.3",
    "pino": "8.11.0",
    "pino-datadog-transport": "1.3.0",
    "pino-pretty": "9.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.7.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@starlightpro/eslint-config-starlight": "^1.0.0",
    "@types/amqplib": "^0.8.2",
    "@types/elasticsearch": "^5.0.40",
    "@types/ioredis": "^4.26.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-helmet": "^6.0.3",
    "@types/koa__cors": "^3.1.1",
    "@types/koa__router": "^8.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^18.7.18",
    "@types/node-fetch": "^2.5.10",
    "@types/pino": "^6.3.8",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cross-env": "^7.0.3",
    "eslint": "8.31.0",
    "http-server": "^0.12.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  },
  "resolutions": {
    "trim-newlines": "^4.0.1"
  }
}
