{"code":"import React, { useCallback } from 'react';\r\nimport { useHistory } from 'react-router-dom';\r\nimport { noop } from 'lodash-es';\r\nimport { Layouts } from \"../../layouts\";\r\nimport { Button as ButtonLayout } from \"./layout/Button\";\r\nexport const Button = ({ iconLeft: IconLeft, iconRight: IconRight, children: baseChildren, disabled, buttonRef, to, full, className, variant, loading, onClick, size = 'medium', type = 'button', value = '', }) => {\r\n    const history = useHistory();\r\n    const handleClick = useCallback((e) => {\r\n        onClick === null || onClick === void 0 ? void 0 : onClick(e, value);\r\n        if (to) {\r\n            history.push(to);\r\n        }\r\n    }, [onClick, value, to, history]);\r\n    return (React.createElement(ButtonLayout, { type: type, ref: buttonRef, full: full, variant: variant, className: className, size: size, onClick: disabled || loading ? noop : handleClick, disabled: disabled, loading: loading },\r\n        React.createElement(Layouts.Padding, { left: \"1\", right: \"1\" },\r\n            React.createElement(Layouts.Flex, { alignItems: \"center\", justifyContent: \"center\", className: \"button-content\" },\r\n                IconLeft ? (React.createElement(Layouts.Margin, { right: \"1\" },\r\n                    React.createElement(IconLeft, null))) : null,\r\n                React.createElement(\"div\", null, baseChildren),\r\n                IconRight ? (React.createElement(Layouts.Margin, { left: \"1\" },\r\n                    React.createElement(IconRight, null))) : null))));\r\n};\r\n//# sourceMappingURL=Button.js.map","references":["/Users/wilson/Documents/Starlight/starlight-shared-libs/node_modules/@types/react/index.d.ts","/Users/wilson/Documents/Starlight/starlight-shared-libs/node_modules/react-router-dom/index.js","/Users/wilson/Documents/Starlight/starlight-shared-libs/node_modules/@types/lodash-es/index.d.ts","/Users/wilson/Documents/Starlight/starlight-shared-libs/packages/shared-compon